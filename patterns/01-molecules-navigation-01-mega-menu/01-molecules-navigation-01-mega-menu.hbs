{{#> molecules-layout-container styleModifier="mega-menu" }}
	<div class="card">
		<div class="card-body">
			<h2>Shop Plans {{> molecules-button styleModifier="btn-secondary" buttonText="Shop Plans"}}</h3>
			<div class="row">
			{{# navItems }}
				{{#unless @index}}
				{{# subNav }}
					<div class="col-sm-4">
						{{> molecules-v-nav }}
					</div>
				{{/ subNav }}
				{{/unless}}
			{{/ navItems }}
			</div>
		</div>
	</div>
{{/ molecules-layout-container}}